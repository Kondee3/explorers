{
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "explorers",
        "transparent": true,
        "width": 800
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "npx tailwindcss build -i ./src/styles.css -o ./dist/output.css && yarn build",
    "beforeDevCommand": "npx tailwindcss build -i ./src/styles.css -o ./dist/output.css && yarn dev",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "targets": "all"
  },
  "identifier": "me.kondee3.explorers",
  "plugins": {},
  "productName": "explorers",
  "version": "0.0.1"
}