{
  "build": {
    "beforeDevCommand": "npx tailwindcss build -i ./src/styles.css -o ./dist/output.css && yarn dev",
    "beforeBuildCommand": "npx tailwindcss build -i ./src/styles.css -o ./dist/output.css && yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "explorers",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "me.kondee3.explorers",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "explorers",
        "width": 800,
        "height": 600
      }
    ]
  }
}
